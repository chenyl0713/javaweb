            // 用密匙初始化Cipher对象
            cipher.init(Cipher.ENCRYPT_MODE, secretKey, new IvParameterSpec(ivs.getBytes(code)));
            // 现在，获取数据并加密
            // 正式执行加密操作
            return new BASE64Encoder().encode(cipher.doFinal(src.getBytes(code)));
        } catch (Exception e) {
            e.printStackTrace();
        }
        return null;
    }


    
